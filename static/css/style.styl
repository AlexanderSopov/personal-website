h1
	font-size 3.2em
	font-family 'Palatino Linotype'
h2
	font-family 'Palatino Linotype'
	font-size 2em
h3
	font-family 'Palatino Linotype'
	font-size 1.5em
p
	font-family 'Josefin Sans', sans-serif
	font-weight 400
	font-size 1.4em
.container
	width width
	margin-left auto
	margin-right auto
header
	display flex
	flew-wrap wrap
	z-index 1
	background: rgba(255,255,255,1);
	width:100%
	position fixed
	top:0px
	height headerHeight
	#logo
		cursor pointer
		display block
		h3
			margin 0px
			padding 20px
	#navbar
		margin 0px
		padding:20px
		button
			border 0px
			background-color inherit
			font-family 'Josefin Sans', sans-serif
			margin-left 5px
			margin-right 5px
			font-size 1.3em
			cursor pointer
.header-img
	margin-top headerHeight
	margin-bottom 100px
	width 100%
	img
		width 100%
	
.page
	text-align center
	margin-bottom:100px
.title
	margin-top 80px
	h1
		margin 20px
.content
	margin-top 0px
#footer
	padding 50px
	text-align center
	color #fff
	background-color #1a1b20
	height 200px
	margin 50px 0px 0px 0px

@media only screen and (min-width 769px)
	.container
		width 750px
		.page .content-wrap
			display flex
			flex-wrap wrap
			.content
				padding: 25px
				width calc(50% - 50px)
				text-align left
			.left
				text-align right
	header #logo
		padding-left 0px
		text-align right
		width: 200px
	.ingress p
		font-size 2em
	
